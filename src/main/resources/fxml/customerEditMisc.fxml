<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import org.controlsfx.control.CheckListView?>

<AnchorPane prefHeight="244.0" prefWidth="1400.0"
	stylesheets="@../style.css" xmlns="http://javafx.com/javafx/8.0.171"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="de.visagistikmanager.view.controller.CustomerEditMiscController">
	<VBox minWidth="1400.0" prefWidth="1400.0">
		<children>
			<Label prefHeight="36.0" prefWidth="1400.0"
				style="-fx-background-color: 45B7AF;" styleClass="subheading" text="Sonstiges"
				textAlignment="RIGHT">
				<font>
					<Font name="System Bold" size="20.0" />
				</font>
				<padding>
					<Insets left="10.0" />
				</padding>
			</Label>
			<HBox prefWidth="1350.0" spacing="20.0">
				<children>
					<VBox prefWidth="221.0">
						<children>
							<Label prefHeight="36.0" prefWidth="207.0" text="Kontaktoptionen"
								textAlignment="CENTER">
								<font>
									<Font name="System Bold" size="16.0" />
								</font>
							</Label>
							<CheckListView fx:id="contactOptions"
								fixedCellSize="25.0" prefHeight="125.0" prefWidth="200.0" />
						</children>
					</VBox>
					<VBox layoutX="25.0" layoutY="10.0" prefWidth="221.0">
						<children>
							<Label prefHeight="36.0" prefWidth="207.0" text="Interessen"
								textAlignment="CENTER">
								<font>
									<Font name="System Bold" size="16.0" />
								</font>
							</Label>
							<CheckListView fx:id="interests" fixedCellSize="25.0"
								prefHeight="125.0" prefWidth="200.0" />
						</children>
					</VBox>
				</children>
				<padding>
					<Insets left="15.0" />
				</padding>
			</HBox>
		</children>
	</VBox>

</AnchorPane>
